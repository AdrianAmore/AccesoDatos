<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida.ae5">
	<class name="Llibre" table="llibres">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="titol" type="string" column="titol" />
		<property name="autor" type="string" column="autor" />
		<property name="any_naiximent" type="string" column="any_naiximent" />
		<property name="any_publicacio" type="string" column="any_publicacio" />
		<property name="editorial" type="string" column="editorial" />
		<property name="numpag" type="string" column="numpag" />
	</class>
</hibernate-mapping>